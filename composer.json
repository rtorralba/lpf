    "require": {
        "php": ">=7.0",
        "league/plates": "^3.1",
        "catfan/Medoo": "^1.5",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    },
    "autoload-dev": {
